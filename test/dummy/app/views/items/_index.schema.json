{
  "summary": "List all items",
  "responses": {
    "200": {
      "description": "A list of items",
      "content": {
        "application/json": {
          "schema": {
            "type": "array",
            "items": {
              "type": "object",
              "required": ["id", "name"],
              "properties": {
                "id": { "type": "integer" },
                "name": { "type": "string" }
              }
            }
          }
        }
      }
    }
  }
}
